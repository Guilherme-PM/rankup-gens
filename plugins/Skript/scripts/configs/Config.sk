# Command for reload

command /reloadconfig:
	permission: *
	permission message: &4❌ &cYou can't do that!
	trigger:
		set {_x} to now
		set {ReloadPlayer} to player
		delete {ReloadTime}
		delete {Config::*}
		send ""
		send "&aClearing old Config..."
		wait 0.5 second
		send "&aSuccess!"
		send ""
		send "&aLoading new config... <##00f818>&l(Gens 3.5)"
		send ""
		send player title "" with subtitle "&aReloading..." for 10 seconds
		execute console command "sk reload /configs/Config.sk"
		while {ReloadTime} is not set:
			add 1 to {_stop}
			if {_stop} > 200:
				send "&cAn error has occured!"
				send player title "" with subtitle "&cError!" for 1 seconds
				delete {ReloadTime}
				delete {ReloadPlayer}
				stop loop
			wait 1 tick
		send ""
		send "&aConfig reloaded in <##FCA805>%difference between {_x} and now%&a!"
		send ""
		send player title "" with subtitle "<##FCA805>Success!" for 1 seconds
		delete {ReloadTime}
		stop